import java.util.Scanner;

public class ArrayRuntime {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int [] marks = new int[5];
		int i, sum=0;
		for (i=0; i<5; i++){
			System.out.println("enter the element in index: " +i);
			marks[i] =sc.nextInt();
		}
		for (i=0; i<5; i++){
			System.out.print(marks[i] + "\t");
		}
		System.out.println();
		for (i=0; i<5; i++){
			sum = sum + marks[i];
		}
	}
}
